$('.patient_list .records').append('<%= j render :partial => "patient", :collection => @paginated_patients, :as => :patient %>');
<% if @paginated_patients.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@paginated_patients) %>');
<% else %>
  $('.pagination').remove();
<% end %>
$(".case_list .select_all").click(function(event) {
  $(this).closest(".case_list").find(".patient_case input[type=checkbox]").attr("checked",true);
});
$(".zoom a").fancybox({
  'hideOnContentClick': true,
  'titlePosition': 'over'
});
