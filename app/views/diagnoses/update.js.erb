$("#form_<%= dom_id(@diagnosis) %>").dialog("close");
$('#<%= dom_id(@diagnosis) %>').replaceWith('<%= escape_javascript(render :partial => "diagnoses/editable_snapshot", :locals => { :diagnosis => @diagnosis }) %>');
$("#<%= dom_id(@diagnosis) %> .dialog-form").dialog({
  autoOpen: false,
  height: 400,
  width: 750,
  modal: true
});
$(".enlarge a").fancybox({
  'hideOnContentClick': true,
  'titlePosition': 'over'
});
