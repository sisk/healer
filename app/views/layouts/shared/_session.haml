#session
  - if user_signed_in?
    = raw "Logged in as #{current_user}. #{link_to("Log Out", destroy_user_session_path)} | #{link_to "My Account", edit_user_registration_path}"
    - if permitted_to? :index, User.new
      = raw " | #{link_to_unless_current "Users", users_path}"
  - else
    = link_to "Log In", new_user_session_path
