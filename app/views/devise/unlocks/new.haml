- content_for :title do
  Resend unlock instructions
- content_for :subnavigation do
  = render :partial => "devise/shared/subnav"
= semantic_form_for(resource, :as => resource_name, :url => unlock_path(resource_name)) do |f|
  = devise_error_messages!
  = f.inputs do
    = f.input :email, :label => 'Email'
  = f.buttons do
    = f.commit_button 'Resend unlock instructions'