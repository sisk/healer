<% if @registrations.present? %>
  $("#day_<%= @day_num %>_room_<%= @room_id %>").html('<%= escape_javascript(render :partial => "schedule/registration_scheduled", :collection => @registrations, :as => :registration) %>');
<% else %>
  $("#day_<%= @day_num %>_room_<%= @room_id %>").html("");
<% end %>
